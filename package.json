{
  "name": "gridplus-sdk",
  "version": "0.9.7",
  "description": "SDK to interact with GridPlus Lattice1 device",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "commit": "git-cz",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint --fix src test --ext .ts",
    "precommit": "npm run lint:fix && npm run test",
    "prepublish": "",
    "postinstall": "",
    "test": "ts-mocha --timeout 120000 -R spec test/testAll.ts --recursive --exit",
    "test-abi": "ts-mocha --timeout 360000 -R spec test/testAbi.ts --recursive --exit",
    "test-eth": "ts-mocha --timeout 180000 -R spec test/testEth.ts --recursive --exit",
    "test-eth-msg": "ts-mocha --timeout 180000 -R spec test/testEthMsg.ts --recursive --exit",
    "test-btc": "ts-mocha --timeout 180000 -R spec test/testBtc.ts --recursive --exit",
    "test-kv": "ts-mocha --timeout 180000 -R spec test/testKv.ts --recursive --exit",
    "test-wallet-jobs": "ts-mocha --timeout 300000 -R spec test/testWalletJobs.ts --recursive --exit",
    "test-sigs": "ts-mocha --timeout 300000 -R spec test/testSigs.ts --recursive --exit"
  },
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/GridPlus/gridplus-sdk.git"
  },
  "dependencies": {
    "aes-js": "^3.1.1",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.0.1",
    "bitwise": "^2.0.4",
    "borc": "^2.1.2",
    "bs58check": "^2.1.2",
    "buffer": "^5.6.0",
    "crc-32": "^1.2.0",
    "elliptic": "6.5.4",
    "eth-eip712-util": "^2.3.1",
    "js-sha3": "^0.8.0",
    "rlp-browser": "^1.0.1",
    "secp256k1": "4.0.2",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/transactions": "^5.5.0",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "acorn": ">=6.4.1",
    "bip32": "^1.0.4",
    "bip39": "^3.0.2",
    "bitcoinjs-lib": "4.0.3",
    "chai": "^4.2.0",
    "cli-interact": "^0.1.9",
    "eslint": "^8.7.0",
    "ethereumjs-util": "^7.1.0",
    "it-each": "^0.4.0",
    "lodash": ">=4.17.21",
    "minimist": ">=0.2.1",
    "mocha": "^5.2.0",
    "random-words": "^1.1.1",
    "readline-sync": "^1.4.9",
    "rlp": "^2.2.7",
    "seedrandom": "^3.0.5",
    "ts-mocha": "^9.0.2",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist"
  ],
  "license": "MIT"
}
